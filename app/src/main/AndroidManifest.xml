<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.litte.ui">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".layout.LinearActivity" />
        <activity android:name=".layout.RelativeActivity" />
        <activity android:name=".layout.AddActivity" />
        <activity android:name=".layout.TableActivity" />
        <activity android:name=".layout.FrameActivity" />
        <activity android:name=".layout.Frame2Activity" />
        <activity android:name=".layout.Frame3Activity" />
        <activity android:name=".layout.GridActivity" />
        <activity android:name=".layout.AbsoluteActivity" />
        <activity android:name=".userinterface.TextViewActivity" />
        <activity android:name=".userinterface.EditTextActivity"></activity>
    </application>

</manifest>